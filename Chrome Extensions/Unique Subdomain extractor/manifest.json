// manifest.json
{
    "manifest_version": 3,
    "name": "Unique Domain Extractor",
    "version": "1.0",
    "description": "Extracts and deduplicates top-level domains from Google search results",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["https://www.google.com/*"],
        "js": ["content.js"]
    }]
}